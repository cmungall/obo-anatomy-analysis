[Term]
id: XAO:1000001
name: NF stage 1
namespace: xenopus_developmental_stage
def: "Nieuwkoop and Faber stage 1. Age: 0 hr. External Stage Criteria: One cell stage, shortly after fertilization. Pigmentation darker ventrally than dorsally." [ISBN:0815318960 "Nieuwkoop and Faber (pg. 163)"]
synonym: "zygote" RELATED []
xref: ZFIN:ZDB-STAGE-010723-4
is_a: XAO:1000012 ! embryo

[Term]
id: XAO:1000002
name: early tailbud
namespace: xenopus_developmental_stage
def: "From Nieuwkoop and Faber stage 22 to the end of stage 28." [ISBN:0815318960 "Nieuwkoop and Faber"]
is_a: XAO:1000007 ! tailbud

[Term]
id: XAO:1000003
name: blastula
namespace: xenopus_developmental_stage
def: "From the 64-cell embryo to Nieuwkoop and Faber stage 9." [ISBN:0815318960 "Nieuwkoop and Faber"]
is_a: XAO:1000012 ! embryo

[Term]
id: XAO:1000004
name: cleavage
namespace: xenopus_developmental_stage
def: "From the 2-cell to the 64-cell embryos." [ISBN:0815318960 "Nieuwkoop and Faber"]
is_a: XAO:1000012 ! embryo

